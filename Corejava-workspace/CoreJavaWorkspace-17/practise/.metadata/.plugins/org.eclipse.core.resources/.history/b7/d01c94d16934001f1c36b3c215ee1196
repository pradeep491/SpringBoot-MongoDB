package com.test;

public class PermutationEx1 {
	public static void main(String[] args) {
		String str = "abc";
		getPermutations(str, "");
		System.out.println("all the permutations of" + str + " have been displayed");
	}

	public static void getPermutations(String str, String ans) {
		if (str.length() == 0) {
			System.out.println(ans);
			return;

		}
		for(int i=0;i<str.length();i++) {
			char ch = str.substring(0, i);
			
		}
	}
}
