package com.test;

import java.util.Arrays;
import java.util.List;
import java.util.Map;

public class ListOfEvenAndOdd {
public static void main(String[] args) {
	List<Integer> list = Arrays.asList(1,4,5,6,7,8,9,12,13,14,15,17,18);
	Map<Boolean,List<Integer>> map = list.stream().collect(Collectors.partitioningBy(i->i%2 == 0));
	System.out.println(map);
	
	Set<Entry<Boolean,List<Integer>>> set = map.entrySet();
	
	for(Entry<Boolean,List<Integer>> entry:set ) {
		
	}
}
}
