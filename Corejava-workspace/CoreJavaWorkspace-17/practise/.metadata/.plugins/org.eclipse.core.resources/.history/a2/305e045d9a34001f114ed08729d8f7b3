package com.test;

import java.util.Arrays;
import java.util.HashSet;
import java.util.List;
import java.util.Set;
import java.util.stream.Collectors;

public class DuplicateIntegersEx {
	public static void main(String[] args) {
		List<Integer> list = Arrays.asList(111, 222, 333, 111, 555, 333, 777, 222);
		Set<Integer> set = new HashSet<>();

		Set<Integer> duplicateInt = set.stream().filter(i -> !set.add(i)).collect(Collectors.toSet());
		duplicateInt.forEach(System.out::println);
	}
}
